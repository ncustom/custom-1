<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dhf.mapper.GenreMapper">

    <select id="selectGenresByCategroyId" parameterType="Integer" resultType="map">
      select *
      from genre
      where categoryId = #{categoryId}
    </select>
</mapper>